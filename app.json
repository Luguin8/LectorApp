{
  "expo": {
    "name": "Florecillas de San Francisco",
    "slug": "florecillas-ebook",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juliana.florecillas",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "edgeToEdgeEnabled": true,
      "versionCode": 9
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      "react-native-google-mobile-ads"{
        "androidAppId": "ca-app-pub-2263615536540210~9197678569",
        "userTrackingUsageDescription": "Esta app utiliza identificadores para ofrecer publicidad personalizada."
      }
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "624fe04b-14b8-427b-97ec-da399d5fcdf6"
      }
    }
  }
}